{
    "id": "nodebb-plugin-carousel",
    "name": "NodeBB Plugin Carousel",
    "description": "NodeBB plugin for beautiful responsive carousel image viewer",
    "url": "https://github.com/mani95lisa/nodebb-plugin-carousel",
    "library": "./index.js",
    "hooks": [
        {
            "hook": "filter:parse.post", "method": "parse", "priority": 6
        }
    ],
    "css": [
        "static/owl.carousel.css",
        "static/owl.theme.css"
    ],
    "scripts": [
        "static/owl.carousel.min.js",
        "static/lib.js"
    ],
    "staticDirs": {
        "owl": "static"
    }

}
